{
  "name": "github-migrate",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx src/server.ts",
    "clean": "rm -rf dist",
    "docker:prepare": "npm run build && cp package*.json dist/ && cp src/Dockerfile dist/ && cp -r src/ui dist/",
    "docker:build": "npm run docker:prepare && docker build -t github-migrate-test ./dist",
    "docker:run": "docker run --rm -p 3000:3000 -v $(pwd)/data:/app/data github-migrate-test",
    "docker": "npm run docker:build && npm run docker:run"
  },
  "keywords": [
    "github",
    "migration",
    "gei"
  ],
  "author": "",
  "license": "ISC",
  "description": "GitHub Enterprise Importer migration dashboard",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/client-ssm": "^3.700.0",
    "@aws-sdk/util-dynamodb": "^3.700.0",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.34",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
